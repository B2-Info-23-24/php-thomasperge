<div class="vehicle-container" data-id="{{ id }}">
  {# Car #}
  <div class="car-container">
    {# Infos #}
    <div class="infos-container">
      <div class="flexbox" style="gap: 0.28rem">
        <img src="{{ logo }}" alt="">
        <div class="name-container">
          <div class="brand">{{ brand }}</div>
          <div class="model">{{ model }}</div>
        </div>
      </div>
    </div>
    {# Image Car #}
    <div class="car-img-container flexbox">
      <img src="{{ modelImage }}" class="car-img" alt="">
    </div>
  </div>
